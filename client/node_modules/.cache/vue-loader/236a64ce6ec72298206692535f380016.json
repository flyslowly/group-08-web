xt: 'Other' }\n      ]\n    }\n  },\n  mounted() {\n    this.getStudents()\n  },\n  methods: {\n    onLogin() {\n      if (this.ssn !== null) {\n        for (var key in this.students) {\n          if (this.students[key].ssn === parseInt(this.ssn)) {\n            this.student = this.students[key]\n            this.show = false\n            this.headShow = true\n            this.fname = this.student.name.fname\n            console.log(this.student.name)\n          }\n        }\n      }\n    },\n    onRegister() {\n      var newStudent = {\n        ssn: this.ssn,\n        name: { fname: this.fname,\n          lname: this.lname },\n        sex: this.sex\n      }\n      Api.post('/students', newStudent)\n        .then(response => {\n          this.students.push(response.data)\n          this.student = response.data\n          this.show = false\n          this.headShow = true\n        })\n        .catch(error => {\n          console.log(error)\n        })\n    },\n    onToRegister() {\n      this.formtype = ''\n    },\n    onToLogin() {\n      this.formtype = 'login'\n    },\n    getStudents() {\n      Api.get('students')\n        .then(reponse => {\n          this.students = reponse.data.students\n        })\n        .catch(error => {\n          this.students = []\n          console.log(error)\n        })\n        .then(() => {\n          // This code is always executed (after success or error).\n        })\n    }\n  }\n}\n</script>\n\n<style>\n.p-3 {\n  text-align: left\n}\n</style>\n"]}]}